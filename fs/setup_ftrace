echo 1 > /proc/sys/kernel/panic
echo 1 > /proc/sys/kernel/panic_on_oops
echo 1 > /proc/sys/kernel/softlockup_panic
dmesg -c
trace-cmd reset
echo function > /sys/kernel/debug/tracing/current_tracer
